# Last modified 2019-09-19
# Dorai Sitaram

digf=~/.config/nvim/after/plugin/moredig.vim

test ! -f $digf && exit

vi -es $digf << EOF

func! TrimDigraphs()
  s/.*DigraphNew\s\+\(..\).*/\=printf("digraph %s %s", submatch(1), char2nr(unicode#Digraph(submatch(1))))
endfunc

g/DigraphNew/ call TrimDigraphs()

sort

x
EOF
